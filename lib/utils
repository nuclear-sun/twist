#! /bin/bash

source ../conf/env

# header protector
[[ -z "${__lib_utils__}" ]] && {
__lib_utils__=a


function log(){
	local datetime=$(date +'%Y-%m-%d %H:%M')
	echo "[${datetime}] $0 $@"
}

function send(){
	echo "$@" >> ${INPUT_CHANEL}
}

function receive(){
	echo 
}

function record_pid(){
	echo "$?" >> ${RUNNING_PID_FILE}
}



}
