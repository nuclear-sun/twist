#! /bin/bash 

# header protector
[[ -z "${__CONFIG_ENV__}" ]] && {
__CONFIG_ENV__=a


RUNTIME_DIR=/tmp/twist
[[ ! -d ${RUNTIME_DIR} ]] && mkdir -p ${RUNTIME_DIR}
INPUT_CHANEL=${RUNTIME_DIR}/input
RUNNING_PID_FILE=${RUNTIME_DIR}/pids

SERVER_IP=localhost
PORT=5001

# deprecated
WATCHED_FILE=file

}
